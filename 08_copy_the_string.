#include <stdio.h>

// Function to copy string
void stringCopy(char source[], char target[]) {
    int i = 0;
    while(source[i] != '\0') {
        target[i] = source[i];
        i++;
    }
    target[i] = '\0';  // null terminate
}

int main() {
    char str1[100], str2[100];
    printf("Enter a string: ");
    gets(str1);

    stringCopy(str1, str2);

    printf("Copied string: %s\n", str2);
    return 0;
}